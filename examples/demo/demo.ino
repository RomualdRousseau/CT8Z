#include <CT8Z.h>

int mixes[9][8] = 
{
  {+256,    0,    0, -256,    0,  +64,    0,    0},
  {   0, +256,    0,    0,    0,    0,    0,    0},
  {   0,    0, +256,    0,    0,    0,    0,    0},
  {   0,    0,    0, +256,    0,    0,    0,    0},
  {   0,    0,    0,    0, +256,    0,    0,    0},
  {   0,    0,    0,    0,    0, +256,    0,    0},
  {   0,    0,    0,    0,    0,    0, +256,    0},
  {   0,    0,    0,    0,    0,    0,    0, +256},
  {   0,    0,    0,    0,    0,    0,    0,    0}
};

int values[9] =
{
  512, 512, 512, 512, 512, 512, 512, 512, 1023
};

void setup()
{
	CT8Z.begin();	
}

void loop()
{
  values[0] = analogRead(A0);
  values[1] = analogRead(A1);
  CT8Z.analogWrite(values, mixes);
}


